#! /bin/bash

services=(
    "product-service" 
    "order-service" 
    "auth-service"
    )

mkdir -p logs/

for service in ${services[@]}; do 
    java -jar build/${service}/*SNAPSHOT.jar --spring.config.location=build/${service}/ > logs/${service}.logs 2>&1 && \
    echo "ğŸŸ¢ Started ${service}" 
done